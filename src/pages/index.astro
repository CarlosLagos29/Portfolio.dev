---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";
import Inicio from "../components/Inicio/Inicio.astro";
import Container from "../components/Reutilizables/Container.astro";
import Experience from "../components/Experience/Experience.astro";
import Proyects from "../components/Proyects/Proyects.astro";
import Footer from "../components/Footer/Footer.astro";
---

<Layout title="Portfolio Carlos Lagos">
	<main class="pt-3">
		<Container id="inicio">
			<Inicio />
		</Container>

		<Container id="experiencia">
			<Experience />
		</Container>

		<Container id="proyects">
			<Proyects />
		</Container>
		<Footer />
	</main>
</Layout>
